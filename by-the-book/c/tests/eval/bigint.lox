var base = 10;

fun zeros(n) {
    var r = [];
    for (var i = 0; i < n; i += 1) {
        r = r + [0];
    }
    return r;
}

fun iszero(a) {
    for (var i = 0; i < #a; i += 1) {
        if (a[i] == 0) {} else { return false; }
    }
    return true;
}

fun fromnum(I) {
    var a = [];
    var i = I | 0;
    if (i == 0) { return [0]; }
    while (i > 0) {
        setArray(a, #a, i % base);
        i = i / base;
    }
    return a;
}

